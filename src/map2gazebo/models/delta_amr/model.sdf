                <samples>1411</samples>
                <resolution>0.17</resolution>
                <min_angle>-2.09</min_angle>
                <max_angle>2.09</max_angle>
              </horizontal>
            </scan>
            <range>
              <min>0.14</min>
              <max>10</max>
              <resolution>0.015</resolution>
            </range>
            <noise>
              <type>gaussian</type>
              <mean>0</mean>
              <stddev>0.01</stddev>
            </noise>
          </ray>
          <plugin name='laserscan_RF' filename='libgazebo_ros_ray_sensor.so'>
            <ros>
              <!-- <namespace>/delta_amr</namespace> -->
              <remapping>~/out:=scan_RF</remapping>
            </ros>
            <output_type>sensor_msgs/LaserScan</output_type>
            <frame_name>scan_RF_link</frame_name>
          </plugin>
        </sensor>
      </link>

