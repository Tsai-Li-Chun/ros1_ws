  </link>
  <link name="body_lower">
    <visual>
      <origin xyz="0 0 0"/>
      <geometry>
        <box size="0.90 0.60 0.15"/>
      </geometry>
      <material name="gray">
        <color rgba="1 1 1 0.8"/>
      </material>
    </visual>
  </link>
  
  <link name="imu_link">
    <inertial>
      <mass value="0.1"/>
      <origin rpy=" 0 0 0" xyz="0.0 0.0 0.0"/>
      <inertia ixx="0.0025" ixy="0" ixz="0" iyy="0.0025" iyz="0" izz="0.0025"/>
